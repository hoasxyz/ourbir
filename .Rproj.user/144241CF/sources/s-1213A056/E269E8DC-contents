\name{solarToLunar}
\alias{solarToLunar}
\title{Convert solar calendar to lunar calendar.}
\usage{
solarToLunar(x, more = FALSE)
}
\arguments{
\item{x}{Date format of string.}

\item{more}{Show more information of the Date}
}
\description{
Convert solar calendar to lunar calendar.
}
\examples{
solarToLunar(today())
solarToLunar("2019-5-23")

}
